{
  "manifest_version": 3,
  "name": "Image Wrangler",
  "version": "1.0",
  "description": "Right click and save images and their url to specific bookmark folders",
  "permissions": [
    "contextMenus",
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
